낸사람
<% @checks.each do |x| %>
    <% if x.money == 1 %>
        <form action="/home/no/<%=x.id%>" mehtod="post">
            <%= x.name %>
            <input type="submit" value="확인">
        </form>
    <% end %>
<% end %>


